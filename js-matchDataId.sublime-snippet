// MATCH DATA-ID
\$('.${1:js-buttonName}').on('click', function () {
    var \$this = \$(this);
    var id = \$this.data('id');
    var \$matchedClass = \$('.${1:js-matchedClass}[data-id=' + id + ']');

    // do something
    \$matchedClass.fadeIn();
});

